    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Debug GPU Go",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "console": "integratedTerminal",
            "env": {
                "GPU_GO_HEARTBEAT_MODE": "long-polling",
                "GPU_GO_ENDPOINT": "http://127.0.0.1:8787"
            },
            "args": [
                "agent", "start"
            ],
            "program": "${workspaceFolder}/cmd/ggo/main.go",
        }
    ]
}